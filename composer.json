{
    "name": "goetas/doctrine2xsd",
    "type": "library",
    "description": "Convert Tool from Doctrine2 XML mapping to XSD XML Schema",
    "keywords": ["xml", "xsd", "doctrine", "xmlschmema"],
    "homepage": "https://github.com/goetas/doctrine2xsd",
    "license": "LGPL",
    "authors": [
        {
            "name": "Asmir Mustafic",
            "email": "goetas@lignano.it"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "2.*",
        "symfony/class-loader": "2.*"
    },
    "autoload": {
        "psr-0": { "Goetas\\DoctrineToXsd": "lib/" }
    },
    "target-dir": "Goetas/DoctrineToXsd",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
